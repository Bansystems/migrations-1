{
	"name": "lorenzo/migrations",
	"description": "Database Migration plugin for CakePHP 3.0 based on Phinx",
	"license": "MIT",
	"type": "cakephp-plugin",
	"extra": {
        	"installer-name": "Migrations"
    	},
	"require": {
		"robmorgan/phinx": "*",
		"composer/installers": "*"
	},
	"require-dev": {
		"cakephp/cakephp": "3.0.x-dev"
	},
	"autoload": {
		"psr-4": {
			"Migrations\\": "src",
			"Migrations\\Test\\": "tests"
		}
	}
}
